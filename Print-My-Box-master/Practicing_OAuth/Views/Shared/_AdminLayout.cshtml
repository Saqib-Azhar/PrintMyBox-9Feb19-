
<!doctype html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <link rel="icon" type="image/png" href="assets/img/favicon.ico">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
    <title>Print My Box</title>
    <meta content='width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=0' name='viewport' />
    <meta name="viewport" content="width=device-width" />
    <!-- Bootstrap core CSS     -->
    <link href="/assets/AdminAssets/css/bootstrap.min.css" rel="stylesheet" />
    <!-- Animation library for notifications   -->
    <link href="/assets/AdminAssets/css/animate.min.css" rel="stylesheet" />
    <!--  Light Bootstrap Table core CSS    -->
    <link href="/assets/AdminAssets/css/light-bootstrap-dashboard.css" rel="stylesheet" />
    <!--  CSS for Demo Purpose, don't include it in your project     -->
    <link href="/assets/AdminAssets/css/demo.css" rel="stylesheet" />
    <!--     Fonts and icons     -->
    <link href="https://maxcdn.bootstrapcdn.com/font-awesome/4.2.0/css/font-awesome.min.css" rel="stylesheet">
    <link href='https://fonts.googleapis.com/css?family=Roboto:400,700,300' rel='stylesheet' type='text/css'>
    <link href="/assets/AdminAssets/css/pe-icon-7-stroke.css" rel="stylesheet" />
    <!--   Core JS Files   -->
    <script src="/assets/AdminAssets/js/jquery-1.10.2.js" type="text/javascript"></script>
    <script src="/assets/AdminAssets/js/bootstrap.min.js" type="text/javascript"></script>
    <!--  Checkbox, Radio & Switch Plugins -->
    <script src="/assets/AdminAssets/js/bootstrap-checkbox-radio-switch.js"></script>
    <!--  Charts Plugin -->
    <script src="/assets/AdminAssets/js/chartist.min.js"></script>
    <!--  Notifications Plugin    -->
    <script src="/assets/AdminAssets/js/bootstrap-notify.js"></script>
    <!-- Light Bootstrap Table Core javascript and methods for Demo purpose -->
    <script src="/assets/AdminAssets/js/light-bootstrap-dashboard.js"></script>
    <!-- Light Bootstrap Table DEMO methods, don't include it in your project! -->
    <script src="/assets/AdminAssets/js/demo.js"></script>
    <link href="/assets/AdminAssets/assets1/css/style.css" rel="stylesheet" />

</head>
<body>
    <div class="navbar navbar-inverse navbar-fixed-top">
        <div class="container">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                <a class="navbar-brand icon-xing-sign" href="/Home/index"><b><strong style="font-family:'Brush Script MT'">Print</strong><small style="font-family:Magneto"><i>MyBox</i></small></b></a>
            </div>
            <div class="navbar-collapse collapse">
                @*@Html.Partial("_MenuPart")*@
                @Html.Partial("_LoginPartial")
            </div>
        </div>
    </div>
    <div class="wrapper">
        <div class="sidebar" data-color="purple" data-image="/assets/AdminAssets/img/sidebar-5.jpg">
            <!--   you can change the color of the sidebar using: data-color="blue | azure | green | orange | red | purple" -->
            <div class="sidebar-wrapper">
                <div class="logo">
                    <a href="/AdminPanel/Panel" class="simple-text">
                        Print My Box
                    </a>
                </div>
                <ul class="nav">
                    <li class="active">
                        <a href="/AdminPanel/Panel">
                            <i class="pe-7s-graph"></i>
                            <p>Admin Dashboard</p>
                        </a>
                    </li>
                    <li>
                        <a href="/AspNetUsers/Index">
                            <i class="pe-7s-users"></i>
                            <p>Manage Admins</p>
                        </a>
                    </li>
                    <li>
                        <a href="/Products/Index">
                            <i class="pe-7s-shopbag"></i>
                            <p>Manage Products</p>
                        </a>
                    </li>
                    <li>
                        <a href="/Categories/Index">
                            <i class="pe-7s-network"></i>
                            <p>Manage Categories</p>
                        </a>
                    </li>
                    <li>
                        <a href="/CategoryTypes/Index">
                            <i class="pe-7s-keypad"></i>
                            <p>Manage Categ. Types</p>
                        </a>
                    </li>
                    <li>
                        <a href="/ProductsReviews/Index">
                            <i class="pe-7s-graph"></i>
                            <p>Products Reviews</p>
                        </a>
                    </li>
                    <li>
                        <a href="/PriceQuotes/Index">
                            <i class="pe-7s-calculator"></i>
                            <p>Price Quote Requests</p>
                        </a>
                    </li>
                    <li>
                        <a href="/ContactUsDatas/Index">
                            <i class="pe-7s-mail-open-file"></i>
                            <p>Contact Us Requests</p>
                        </a>
                    </li>
                </ul>
            </div>
        </div>
        <div class="main-panel">
            <div class="content">
                <div class="container-fluid">
                    <div class="card">
                        <div class="content">
                            <div class="row">
                                <div class="col-md-12">
                                    @RenderBody()
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <footer class="footer">
            <div class="container-fluid right">
                &copy; PrintMyBox.com Developed by <a href="http://www.facebook.com/saqib.abdullah.azhar" target="_blank" title="Developer">Saqib Abdullah Azhar</a>
            </div>
        </footer>
    </div>
</body>
@Scripts.Render("~/bundles/jquery")
@Scripts.Render("~/bundles/bootstrap")
@RenderSection("scripts", required: false)
</html>